<div class="search-form-control form-group">
  <%= form_tag users_path, method: :get do %>
    <%= text_field_tag :query, params[:query], class: "form-control", placeholder: "Find a star" %>
    <%= submit_tag "Search", class: "btn btn-primary" %>
  <% end %>
</div>
