
 <div class="container pt-5">
<div class="row justify-content-center">
 <div class="col-12 col-lg-8">

<% @users.each do |user| %>
  <%= link_to user.name, user_path(user) %>
  <p>Bio: <%= user.bio %></p>
  <%= cl_image_tag user.photo, height: 150, width: 200, crop: :fill %>
  <p>-------------</p>
<% end %>

</div>
</div>
</div>
